{
  "name": "scheduler",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Tim Feeley",
    "email": "tim@timfeeley.com"
  },
  "scripts": {
    "build": "next build",
    "db:generate": "drizzle-kit generate:sqlite && drizzle-kit generate:pg",
    "db:init": "tsx scripts/init-db.ts",
    "db:push": "drizzle-kit push:sqlite",
    "db:push:pg": "drizzle-kit push:pg",
    "dev": "next dev --turbopack",
    "format": "sort-package-json && prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --write",
    "format:check": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --check",
    "lint": "eslint",
    "test": "NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "lint:fix": "pnpm lint --fix",
    "start": "next start"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.2",
    "envin": "^1.1.3",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "postgres": "^3.4.7",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.3.1",
    "tsdav": "^2.1.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.3.1",
    "@eslint/eslintrc": "^3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@jest/globals": "^30.0.4",
    "@next/eslint-plugin-next": "^15.3.5",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/better-sqlite3": "^7.6.13",
    "@types/luxon": "^3.6.2",
    "@types/node": "^24.0.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-test-renderer": "^19.1.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.30.1",
    "eslint-config-next": "15.3.5",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-import-x": "^4.16.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "jest": "^30.0.4",
    "jest-environment-jsdom": "^30.0.4",
    "luxon": "^3.6.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "sort-package-json": "^3.4.0",
    "tailwindcss": "^4.1.11",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "undici": "^7.11.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.74"
  }
}
