{
  "name": "scheduler",
  "version": "0.1.0",
  "private": true,
  "author": {
    "name": "Tim Feeley",
    "email": "tim@timfeeley.com"
  },
  "scripts": {
    "build": "pnpm db:init && next build",
    "db:generate": "drizzle-kit generate:sqlite && drizzle-kit generate:pg",
    "db:init": "tsx scripts/init-db.ts",
    "db:push": "drizzle-kit push:sqlite",
    "db:push:pg": "drizzle-kit push:pg",
    "dev": "tsx scripts/validate-setup.ts && next dev --turbopack",
    "dev:skip-validation": "next dev --turbopack",
    "env:generate": "tsx scripts/generate-env.ts",
    "format": "sort-package-json && prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --write",
    "format:check": "prettier '**/*.{cjs,mjs,ts,tsx,md,json}' --ignore-path ../.gitignore --ignore-unknown --no-error-on-unmatched-pattern --check",
    "lint": "eslint",
    "lint:fix": "pnpm lint --fix",
    "pre-commit": "pnpm format:check && npx tsc -p tsconfig.json --noEmit && npx eslint . --ext .js,.jsx,.ts,.tsx && pnpm test --silent",
    "start": "next start",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "validate-setup": "tsx scripts/validate-setup.ts"
  },
